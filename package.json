{
  "name": "partmonger-inventory",
  "description": "PartMonger Inventory Tracker",
  "version": "0.1.0",
  "author": "Justin Linn <linnjsconsulting@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GovCIO/partmonger-invetory"
  },
  "keywords": [
    "inventory",
    "part",
    "parts",
    "monger",
    "partmonger",
    "management"
  ],
  "scripts": {
    "postinstall": "husky install",
    "start": "yarn develop",
    "develop": "cross-env NODE_OPTIONS=\"-r esm\" gatsby develop -H 0.0.0.0",
    "open": "cross-env NODE_OPTIONS=\"-r esm\" gatsby develop -H 0.0.0.0 -o",
    "build": "cross-env NODE_OPTIONS=\"-r esm\" gatsby build",
    "serve": "cross-env NODE_OPTIONS=\"-r esm\" gatsby serve -o -p 8080",
    "playground": "cross-env NODE_OPTIONS=\"-r esm\" GATSBY_GRAPHQL_IDE=playground  gatsby develop",
    "storybook": "start-storybook -p 9000 -c .storybook",
    "build:storybook": "build-storybook -c .storybook -o storybook",
    "build:all": "gatsby build && build-storybook -c .storybook -o storybook",
    "clean": "gatsby clean",
    "lint": "eslint src -c .eslintrc.js --ext js,jsx",
    "lint:fix": "eslint src -c .eslintrc.js --fix --ext js,jsx"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "gatsby": "^3.11.1",
    "gatsby-background-image": "^1.5.3",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-image": "^1.11.0",
    "gatsby-plugin-manifest": "^3.11.0",
    "gatsby-plugin-offline": "^4.11.0",
    "gatsby-plugin-react-helmet": "^4.11.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-root-import": "^2.0.6",
    "gatsby-plugin-sharp": "^3.11.0",
    "gatsby-plugin-styled-components": "^4.11.0",
    "gatsby-plugin-transition-link": "^1.20.5",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-source-filesystem": "^3.11.0",
    "gatsby-transformer-sharp": "^3.11.0",
    "gbimage-bridge": "^0.1.4",
    "gsap": "^3.7.1",
    "isomorphic-fetch": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.12.2",
    "react-share": "^4.4.0",
    "react-spring": "^9.2.4",
    "styled-components": "^5.3.0",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addons": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.13.2",
    "cross-env": "^7.0.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "esm": "^3.2.25",
    "husky": "^7.0.1",
    "prettier": "^2.3.2"
  }
}
